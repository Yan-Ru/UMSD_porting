{
    "configurations": [
        {
            "name": "C/C++: gcc build and debug active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [
                {"name": "MSD_ROOT", "value": "${env:PWD}"},
                {"name": "MSD_USER_BASE", "value": "${env:MSD_ROOT}"},
                {"name": "MSD_PROJ_NAME", "value": "msdDrv"},
                {"name": "MSD_TOOL_DIR", "value": "${env:MSD_USER_BASE}/tools"},
                {"name": "RELEASE", "value": "YES"},
                {"name": "OS_RUN", "value": "LINUX"},
                {"name": "TOPAZ_BUILD", "value": "YES"},
                {"name": "PERIDOT_BUILD", "value": "YES"},
                {"name": "AGATE_BUILD", "value": "YES"},
                {"name": "PEARL_BUILD", "value": "YES"},
                {"name": "AMETHYST_BUILD", "value": "YES"},
                {"name": "OAK_BUILD", "value": "YES"},
                {"name": "SPRUCE_BUILD", "value": "YES"},
                {"name": "BONSAIZ1_BUILD", "value": "YES"},
                {"name": "FIR_BUILD", "value": "YES"},
                {"name": "BONSAI_BUILD", "value": "YES"}
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++: gcc build active file",
            "miDebuggerPath": "/usr/bin/gdb"
        },
